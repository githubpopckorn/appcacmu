= form_for @withoutgood_stage do |f|
  - if @withoutgood_stage.errors.any?
    #error_explanation
      %h2= "#{pluralize(@withoutgood_stage.errors.count, "error")} prohibited this withoutgood_stage from being saved:"
      %ul
        - @withoutgood_stage.errors.full_messages.each do |message|
          %li= message

  .field
    = f.label :name
    = f.text_field :name
  .field
    = f.label :months
    = f.text_field :months
  .field
    = f.label :days
    = f.text_field :days
  .actions
    = f.submit 'Save'
